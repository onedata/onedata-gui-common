<div class="workflow-visualiser">
  {{#each (array
    "pending"
    "resuming"
    "scheduled"
    "preparing"
    "enqueued"
    "active"
    "stopping"
    "interrupted"
    "paused"
    "cancelled"
    "skipped"
    "finished"
    "failed"
    "crashed"
    "unknown"
    "randomValue"
  ) as |status|}}
  <p>
    Status {{status}}:
    {{workflow-visualiser/lane/run-indicator status=status runNumber=1}}
    {{workflow-visualiser/lane/run-indicator
      status=status
      runNumber=3
      originRunNumber=1
      isActive=true
      class="clickable"
    }}
  </p>
  {{/each}}

  {{#each (array 1 20 456 -1 "randomValue") as |runNumber|}}
    <p>
      Run number {{runNumber}}:
      {{workflow-visualiser/lane/run-indicator runNumber=runNumber}}
    </p>
  {{/each}}
</div>
