{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/tmp/**": true,
    "**/dist/**": true,
    "**/rel/**": true,
    "**/node_modules/**": true,
    "**/bower_components/**": true
  },
  "editor.tabSize": 2,
  "editor.rulers": [
    90
  ],
  "files.insertFinalNewline": true,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true
  },
  "[handlebars]": {
    "files.insertFinalNewline": false
  },
  "seito-openfile.wordbound": "[\\s\\\"\\'\\>\\<#;?{}.(]",
  "seito-openfile.leadingPathMapping": {
    "oneprovider-gui": "",
    "onezone-gui": "",
    "onepanel-gui": ""
  }
}
