{{one-draggable-object-target
  validateDragEvent=(action "validateDragEvent")
  action=(action "acceptDraggedElement")
}}
{{#if (not-eq arrowType "none")}}
  <div class="arrow">
    <div class="line"></div>
    <div class="arrowhead"></div>
  </div>
{{/if}}
{{#if allowAdding}}
  {{one-icon
    class="add-block-action-trigger"
    icon="add-filled"
    click=(action "addLaneElement")
  }}
{{/if}}
{{yield}}
