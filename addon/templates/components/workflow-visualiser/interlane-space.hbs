{{#if (eq mode "edit")}}
  {{one-draggable-object-target
    validateDragEvent=(action "validateDragEvent")
    action=(action "acceptDraggedElement")
  }}
  <div
    class="{{createLaneAction.className}} clickable"
    {{action createLaneAction.executeCallback}}
  >
    {{one-icon icon=createLaneAction.icon}}
  </div>
{{/if}}
{{yield}}
