{{#if (and (not isReadOnly) (not hideActions))}}
  <div class="list-header">
    {{#one-button
      class="add-series-btn"
      type="primary"
      size="sm"
      onClick=(action "addSeries")
    }}
      {{one-icon icon="plus"}}
      <span class="text">{{tt this "addSeries"}}</span>
    {{/one-button}}
  </div>
{{/if}}
{{atm-workflow/charts-dashboard-editor/chart-editor/elements-list
  itemModels=itemModels
  allowDragDrop=(not hideActions)
  hideActions=hideActions
  actionsFactory=actionsFactory
  isReadOnly=isReadOnly
}}
