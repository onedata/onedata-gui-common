<div class="space-icon item-icon-container">
  {{one-icon icon=_iconName class="item-icon"}}
</div>
{{#truncated-string class="space-label"}}
  {{#link-to
    "provider-redirect"
    providerId
    (query-params space_id=(model-routable-id space.id))
    class="space-redirect-link"
  }}
    {{name-conflict item=space}}
  {{/link-to}}
{{/truncated-string}}
<div class="space-size">{{bytes-to-string _supportSize}}</div> 
