{{#if item.icon}}
  {{one-icon class=iconClass icon=item.icon}} 
{{/if}}
<span class="one-label">{{item.label}}</span>
{{#if item.warningMessage}}
  {{one-icon class="warning-icon" icon="sign-warning"}}
  {{#unless media.isMobile}}
    {{one-tooltip
      title=item.warningMessage
      triggerElement=".warning-icon"
      triggerEvent="hover"
    }}
  {{/unless}}
{{/if}}
